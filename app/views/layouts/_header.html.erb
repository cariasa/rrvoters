<header class="navbar navbar-fixed-top navbar-inverse">
  <div class="container">
    <%= link_to "registro", root_path, id: "logo" %>
    <nav>
      <ul class="nav navbar-nav navbar-right">
        <li><%= link_to "Inicio",   root_path %></li>
        <li><%= link_to "Ayuda",   help_path %></li>
        <li><%= link_to "Acerca", about_path %></li>

        <% if user_signed_in? %>
          
          <li><%= link_to "Editar perfil: #{current_user.email}", edit_user_registration_path  %> </li>
          <li><%= link_to "Salir", destroy_user_session_path, method: :delete  %> </li>
        <% else %>
          <li><%= link_to "Crear cuenta", new_user_registration_path   %> </li>
          <li><%= link_to "Ingresar", new_user_session_path   %> </li>
        <% end %>

      </ul>
    </nav>
  </div>
</header>
