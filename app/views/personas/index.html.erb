<p id="notice"><%= notice %></p>

<h1>Listing Personas</h1>

<table>
  <thead>
    <tr>
      <th>Identidad persona</th>
      <th>Nombres persona</th>
      <th>Apellidos persona</th>
      <th>Telefono persona</th>
      <th>Email persona</th>
      <th>Barrio colonia</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @personas.each do |persona| %>
      <tr>
        <td><%= persona.identidad_persona %></td>
        <td><%= persona.nombres_persona %></td>
        <td><%= persona.apellidos_persona %></td>
        <td><%= persona.telefono_persona %></td>
        <td><%= persona.email_persona %></td>
        <td><%= persona.barrio_colonia %></td>
        <td><%= link_to 'Show', persona %></td>
        <td><%= link_to 'Edit', edit_persona_path(persona) %></td>
        <td><%= link_to 'Destroy', persona, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Persona', new_persona_path %>
